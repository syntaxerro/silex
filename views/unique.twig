<!DOCTYPE html>
<html><head>
<script src="/js/jq2.1.3.js"></script>
<link rel="stylesheet" href="/css/bootstrap.min.css"/>
<script src="/js/bootstrap.min.js"></script>
<link rel="shortcut icon" href="/img/inj.png"/>
<style>
body { padding-top: 70px; }
</style>
<title>SQL History</title>
</head>
<body>
	<nav class="navbar navbar-default navbar-fixed-top">
		<div class="container">
			<ul class="nav nav-pills nav-justified" style="margin-top: .4em;"> 
			{% if id==1 %}
				<li role="presentation" class="active"><a href="/silex/unique/1">mySQL Injection #1</a></li>
				<li role="presentation"><a href="/silex/unique/2">mySQL Injection #2</a></li>
				<li role="presentation"><a href="/silex/unique/3">mySQL Injection #3</a></li>
			{% endif %}

			{% if id==2 %}
				<li role="presentation"><a href="/silex/unique/1">mySQL Injection #1</a></li>
				<li role="presentation" class="active"><a href="/silex/unique/2">mySQL Injection #2</a></li>
				<li role="presentation"><a href="/silex/unique/3">mySQL Injection #3</a></li>
			{% endif %}

			{% if id==3 %}
				<li role="presentation"><a href="/silex/unique/1">mySQL Injection #1</a></li>
				<li role="presentation"><a href="/silex/unique/2">mySQL Injection #2</a></li>
				<li role="presentation" class="active"><a href="/silex/unique/3">mySQL Injection #3</a></li>
			{% endif %}
			</ul>
		</div>	
	</nav>
{% if id==1 %}
<div class="panel panel-success" style="margin: .5em;">
{% endif %}

{% if id==2 %}
<div class="panel panel-info" style="margin: .5em;">
{% endif %}

{% if id==3 %}
<div class="panel panel-warning" style="margin: .5em;">
{% endif %}

<div class="panel-heading" style="display: flex; justify-content: space-between;">
		<span id="head_t" style="font-weight: 600;">Lista unikalnych adres&#xF3;w</span>
		<span style='font-weight: 600;'>Liczba pr&#xF3;b</span>
</div>
<div class="panel-body">

<div class="list-group">
{% for item in all %}
<a href="/silex/{{id}}/ip/{{item.ip}}" class="list-group-item" style="display: none; justify-content: space-between;" id="list{{item.i}}">

<span style="font-size: 1.1em;">{{item.ip}}</span>

{% if item.bot == true %}
<span class="label label-danger" style="display: flex; align-items: center;">{{item.licznik}}</span>
{% endif %}

{% if item.bot == false %}
<span class="label label-success" style="display: flex; align-items: center;">{{item.licznik}}</span>
{% endif %}

</a>
{% endfor %}
</div>
</div>
</body>
<script>

var i=0;
$("#head_t").html($("#head_t").html()+": "+$(".list-group-item").length);
function petla() {
	if(i < $(".list-group-item").length) setTimeout(function() {
		$("#list"+i).css('display', 'flex').hide().fadeIn(410);
		i++; petla();
	}, 50);
}
petla();
</script>
</html>